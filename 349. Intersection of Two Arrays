class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer>set1 = new HashSet<>();
        for(int n:nums1){
            set1.add(n);
        }
        Set<Integer>set2 = new HashSet<>();
        for(int m:nums2){
            if(set1.contains(m)){
                set2.add(m);
            }
        }
        int a[] = new int[set2.size()];
        int p = 0;
        for(int k : set2){
            a[p++] = k;
        }
        return a;
    }
}
