class Solution {
    public int countElements(int[] nums, int k) {
        Arrays.sort(nums);
        if(k == 0)return nums.length;
        if(k >= nums.length)return 0;

        int t = nums[nums.length - k];
        int c = 0;
        for(int num : nums){
            if(num < t)c++;
        }
        return c;
    }
}
