class Solution {
    public int[] constructArray(int n, int k) {
        int[] ans = new int[n];
        int idx = 0;

        for(int i=1;i<=n-k;i++){
            ans[idx++] = i;
        }
        boolean h = true;
        int l = n - k + 1 , high = n;
        while(idx < n){
            if(h){
                ans[idx++] = high--;
            }else{
                ans[idx++] = l++;
            }
            h = !h;
        }
        return ans;
    }
}
