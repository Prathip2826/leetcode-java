class Solution {
    public int countPrefixSuffixPairs(String[] words) {
        int c = 0;
        int n = words.length;
        for(int i = 0; i < n; i++){
            String a = words[i];
            for(int j = i+1; j < n; j++){
                String b = words[j];
                if(a.length() > b.length())continue;
                
                if(b.startsWith(a) && b.endsWith(a)){
                    c++;
                }
            }
        }
        return c;
    }
}
